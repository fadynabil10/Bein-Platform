<script type="text/javascript" src="../../../https@2wlxixn9sua2wdd093opll91-wpengine.netdna-ssl.com/wp-content/themes/bein/assets/Silverlight.js"></script>

<script type="text/javascript">

    function onSilverlightError(sender, args) {
        //alert('herhe1');
        //console.log('ARGS'+args);

        var appSource = "";
        if (sender != null && sender != 0) {
            appSource = sender.getHost().Source;
        }

        var errorType = args.ErrorType;
        var iErrorCode = args.ErrorCode;

        if (errorType == "ImageError" || errorType == "MediaError") {
            return;
        }

        var errMsg = "Erreur non gérée de l'application Silverlight " + appSource + "\n";

        errMsg += "Code : " + iErrorCode + "    \n";
        errMsg += "Catégorie : " + errorType + "       \n";
        errMsg += "Message : " + args.ErrorMessage + "     \n";

        if (errorType == "ParserError") {
            errMsg += "Fichier : " + args.xamlFile + "     \n";
            errMsg += "Ligne : " + args.lineNumber + "     \n";
            errMsg += "Position : " + args.charPosition + "     \n";
        }
        else if (errorType == "RuntimeError") {
            if (args.lineNumber != 0) {
                errMsg += "Ligne : " + args.lineNumber + "     \n";
                errMsg += "Position : " + args.charPosition + "     \n";
            }
            errMsg += "Nom de la méthode " + args.methodName + "     \n";
        }

        throw new Error(errMsg);
    }
</script>

<script type="text/javascript">


        var ICL_LANGUAGE_CODE = 'ar';
        var slCtl = null;
        var authToken = '';

        var openingID = 17;


        if(window.location.hash) {
            var hash = window.location.hash.substring(1); //Puts hash in variable, and removes the # character
            hash = hash.toLowerCase();
            if(jQuery('#' + hash).length){
                openingID = jQuery('#' + hash).data('cid');
            }
        }


        jQuery('document').ready(function(){

            if (sessionStorage.getItem("newAuthToken") === null) {
                jQuery('.logged_out_channel').show();
                jQuery('.logged_in_channel').hide();
            }else{
                jQuery('.logged_out_channel').hide();
                jQuery('.logged_in_channel').show();
            }

        });


        if (sessionStorage.getItem("newAuthToken") === null) {
            isChannelFTA(true);
            path = "path=/"+";";
            document.cookie = "_wp_session=;"+ path +"expires=Thu, 01 Jan 1970 00:00:01 GMT;";
            console.log('in auth token null block'+document.cookie);
            authToken = '';
            jQuery('.logged_out_channel').show();
            jQuery('.logged_in_channel').hide();
        }else{
            authToken = sessionStorage.getItem("newAuthToken");
            console.log('authToken:'+authToken);
            jQuery('.logged_out_channel').hide();
            jQuery('.logged_in_channel').show();
        }

        function pluginLoaded(sender) {
            slCtl = sender.getHost();
            window.setTimeout(function(){ slCtl.Content.scriptApi.Play(openingID, authToken, ICL_LANGUAGE_CODE); },2500);
        }
        function Channel(number, language) {
            //lstring = "slCtl.Content.scriptApi.Play("+number+", "+authToken+", "+language+");";
           // console.log(lstring);
            slCtl.Content.scriptApi.Play(number, authToken, language);
            //jQuery('body').animate({scrollTop:0}, '500');
            window.scrollTo(0,0);
        }

        function isChannelFTA(loggedOut) {

            if(loggedOut) {
                if(window.location.hash) {
                    var hash = window.location.hash.substring(1); //Puts hash in variable, and removes the # character
                    hash = hash.toLowerCase();
                    if(hash != 'beinsports' && hash != 'beinsportsnews'){
                        if(ICL_LANGUAGE_CODE == 'en'){
                            window.location.replace('https://connect.bein.net/en/login/');
                        }else if(ICL_LANGUAGE_CODE == 'fr'){
                            window.location.replace('https://connect.bein.net/fr/se-connecter/');
                        }else{
                            window.location.replace('https://connect.bein.net/ar/login-ar/');
                        }
                    }
                }
            }

        }

</script>
