        jQuery('.incompatible_browser, .player-wrapper, .channels-topper, .channels, .separator').css('display', 'none');
    } else {
        console.log("browser");
        var isFirefox = typeof InstallTrigger !== 'undefined';
        var isIE = /*@cc_on!@*/false || !!document.documentMode;
        var isEdge = (navigator.appVersion.indexOf('Trident') >= -1) ? false : true;
        var isSafari = navigator.userAgent.indexOf("Safari") > -1;
        //var isChrome = navigator.userAgent.indexOf('Chrome') > -1;


        if(isFirefox || isIE || isEdge || isSafari) {
            document.getElementsByClassName('incompatible_browser')[0].style.display='none';
            document.getElementsByClassName('player-wrapper')[0].style.display='block';
        } else {
            document.getElementsByClassName('incompatible_browser')[0].style.display='block';
            document.getElementsByClassName('player-wrapper')[0].style.display='none';
        }

    }

    //jQuery('document').ready(function(){

        //function loadClickChannels() {
            //console.log("scroll to top");
            //jQuery('.channels > button').click(function() {

            //});
        //}

        //loadClickChannels();

    //});


</script>









